package bytestream;

public class Employee {
	private int no;
	private String name;
	private String job;
	private int daptno;
	private double sal;
	
	public Employee(int no, String name, String job, int daptno, double sal) {
		super();
		this.no = no;
		this.name = name;
		this.job = job;
		this.daptno = daptno;
		this.sal = sal;
	}
	
	
}
